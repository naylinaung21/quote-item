<main class="container">
  
  <div class="header">
    <h1>Poem Books</h1>
    <%= link_to "New Poem Book", new_poem_book_path, data: { turbo_frame: dom_id(PoemBook.new) }, class: "btn btn--primary" %>
  </div>

  <%= turbo_frame_tag PoemBook.new %>

  <%= turbo_frame_tag "poem_books" do %>
    <%= render @poem_books %>
  <% end %>

</main>
